{
    "implicitDependencies": {
        "angular.json": "*",
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        ".eslintrc.js": "*",
        "nx.json": "*",
        "karma.*.js": "*",
        "tsconfig.*.json": "*",
        "tsconfig.json": "*"
    },
    "npmScope": "angular-open-source-starter",
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nx-cloud/runners/firebase",
            "options": {
                "cacheableOperations": ["build", "test", "lint"],
                "parallel": 1
            }
        }
    },
    "affected": {
        "defaultBase": "origin/main"
    },
    "workspaceLayout": {
        "libsDir": "projects",
        "appsDir": "projects"
    },
    "defaultProject": "demo"
}
